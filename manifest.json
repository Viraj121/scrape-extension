{
  "manifest_version": 3,
  "name": "Zigger-Scarpper",
  "version": "1.0",
  "description": "This Chrome extension allows users to scrape tables from websites and export the data into a CSV format",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "32": "./assets/icon2.png",
      "48": "./assets/icon2.png",
      "128": "./assets/icon2.png"
    }
  },
  "icons": {
    "32": "./assets/icon2.png",
    "48": "./assets/icon2.png",
    "128": "./assets/icon2.png"
  }
}