{
  "manifest_version": 3,
  "name": "Table Scraper",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads"
  ],
  "action": {
    "default_icon": {
      "16": "./assets/icon.png",
      "48": "./assets/icon.png",
      "128": "./assets/icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "./assets/icon.png"
  }
}